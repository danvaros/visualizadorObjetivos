      $('#sel_estados').change(function(){

      for(var j=1;j<=40;j++){
        $('#anio_'+j).removeClass('active_anio');
        $('#anio_'+j).addClass('inactive_anio');
      }

      $('#anio_1').addClass('active_anio');
