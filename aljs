              layer.on({
                mouseover: highlightFeature,
                mouseout: resetHighlight,
                mousedown: function(e)
                {
                  if (locked == true) {
                    locked = false;
                    (function ($) {
                      $(".info2").css("border","none");
                      $(".infos").css("border","none");
