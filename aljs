            arre.push(i)
          }

          table2 = $('#miTabla2').DataTable({
            scrollY:        "600px",
            scrollX:        true,
            scrollCollapse: true,
            paging:         false,
            searching: false,
            aoColumnDefs: [{
              'bSortable': false,
              'aTargets': arre
            }],
            fixedColumns:{
              leftColumns: 1
            }
          });
        }
      }
      else
      $('#datos_calculo_2').hide();
    });

    put_anios();

